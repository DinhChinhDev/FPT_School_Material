@page
@model PRN221_DinhChinh_Ass3.WebApp.Pages.Home.IndexUserModel

@{
    ViewData["Title"] = "IndexUser";
}
<link rel="stylesheet" href="~/css/StyleSheet1.css">
<h1>Index</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Ship[0].DateOrder)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ship[0].DateShip)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ship[0].Book)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ship[0].Book.Price)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ship[0].UserShip)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Ship) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DateOrder)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateShip)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Book.BookName)
            </td>
            <td>
                    @Html.DisplayFor(modelItem => item.Book.Price)
            </td>
            <td>
                    @Html.DisplayFor(modelItem => item.UserShip.UserName)
            </td>
            
        </tr>
}
    </tbody>
</table>
